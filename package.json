{
  "name": "wareki",
  "version": "0.1.3",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --extensions \".ts\" --out-dir lib",
    "check": "tsc",
    "test": "ava",
    "test:cov": "nyc ava && nyc report | coveralls",
    "lint": "tslint -c tslint.json './*.ts'",
    "lint:fix": "tslint -c tslint.json './*.ts' --fix",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tohashi/wareki.git"
  },
  "author": "tohashi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tohashi/wareki/issues"
  },
  "homepage": "https://github.com/tohashi/wareki#readme",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-typescript": "7.3.3",
    "@babel/register": "7.4.0",
    "ava": "1.3.1",
    "babel-plugin-add-module-exports": "1.0.0",
    "coveralls": "3.0.3",
    "nyc": "12.0.2",
    "prettier": "1.16.4",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.3333"
  },
  "files": [
    "lib"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "match": "*",
    "failFast": false,
    "timeout": "10000",
    "serial": false,
    "concurrency": "5",
    "verbose": true,
    "tap": false
  },
  "nyc": {
    "include": [
      "./src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "text-lcov"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": false,
    "all": true
  }
}
